  <rdf:Property rdf:about="https://ontology.swissartresearch.net/aaao/ZP103i_is_social_relation_target_ascribed_by">
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E39_Actor"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property is used to indicate the person with whom the subject of the social relation status has the social relation which they have.</rdfs:comment>
    <rdfs:label xml:lang="en">is social relation target ascribed by</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP103i</skos:notation>

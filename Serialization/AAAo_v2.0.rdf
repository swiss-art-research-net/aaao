    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property is used to indicate the actor upon whom an obligation is generated when two parties reach a promissory agreement.</rdfs:comment>
    <rdfs:label xml:lang="en">was promisor in</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP118i</skos:notation>
    <rdfs:range rdf:resource="https://ontology.swissartresearch.net/aaao/ZE46_Promisory_Agreement"/>
    <rdfs:subPropertyOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/P14i_performed"/>

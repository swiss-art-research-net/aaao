  <rdf:Property rdf:about="https://ontology.swissartresearch.net/aaao/ZP104i_is_social_relation_ascribed_by">
    <rdfs:domain rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E55_Type"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property is used to indicate the specific type of social relation status which is indicated as holding between the subject individual and their ascribed social relative.</rdfs:comment>
    <rdfs:label xml:lang="en">is social relation ascribed by</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP104i</skos:notation>

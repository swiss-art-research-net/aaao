  <rdf:Property rdf:about="https://ontology.swissartresearch.net/aaao/ZP117i_was_promisee_in">
    <rdfs:domain rdf:resource="https://ontology.swissartresearch.net/aaao/ZE36_Persons"/>
    <rdfs:comment xml:lang="en">This property is the inverse of the forward property described in the scope note that follows. This property is used to indicate the persons to whom a promise is made, when two parties reach a promissory agreement.</rdfs:comment>
    <rdfs:label xml:lang="en">was promisee in</rdfs:label>
    <skos:notation rdf:datatype="http://www.w3.org/2001/XMLSchema#string">ZP117i</skos:notation>
